{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": false,
        "lib": [ "es2015" ],
        "outDir": "../lib"
    },
    "exclude": [
        "node_modules"
    ]
}
